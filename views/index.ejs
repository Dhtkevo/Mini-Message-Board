<html>
  <body>
    <% for (let message of messages) { %>
        <h2><%= message.user %></h1>
        <p><%= message.text %></p>
        <p><%= message.added %></p>
        <form method="GET" action="/<%= message.id %>" >
          <button type="submit">Open</button>
      </form>
    <% } %>
  </body>
</html>
